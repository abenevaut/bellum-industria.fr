                      %li
                        %a{"data-toggle" => "tab", :href => "#tab-nvd3-area"}
                          %span Stacked Area Charts
                      %li
                        %a{"data-toggle" => "tab", :href => "#tab-nvd3-line2"}
                          %span Line Charts with area
                    / Tab panes
                    .tab-content.no-padding
                      #tab-nvd3-line.tab-pane.active
                        #nvd3-line.line-chart.m-t-30.text-center{"data-x-grid" => "false"}
                          %svg
                      #tab-nvd3-area.tab-pane
                        #nvd3-area.line-chart.m-t-30.text-center{"data-x-grid" => "false"}
                          %svg
                      #tab-nvd3-line2.tab-pane
                        #nvd3-line2.line-chart{"data-area-color" => "master", "data-line-color" => "master", "data-point-color" => "white", "data-points" => "true", "data-stroke-width" => "1", "data-x-grid" => "false"}
                          %svg
