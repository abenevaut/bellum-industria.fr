{
  "name": "cvepdb/cms",
  "description": "A Laravel Framework based CMS.",
  "keywords": [
    "framework",
    "laravel",
    "cms"
  ],
  "license": "GPL-3.0",
  "type": "project",
  "repositories": [
    {
      "type": "composer",
      "url": "http://packages.cvepdb.fr"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "composer/composer": "^1.0",
    "composer/installers": "~1.0",
    "laravel/framework": "5.3.*",
    "doctrine/cache": "^1.6",
    "doctrine/dbal": "~2.3",
    "nesbot/carbon": "^1.21",
    "league/fractal": "^0.14.0",
    "prettus/l5-repository": "^2.1",
    "spatie/laravel-backup": "3.8.*",
    "cvepdb/laravel-widgets": "dev-master",
    "cvepdb/laravel-themes": "dev-master",
    "cvepdb/laravel-modules": "dev-master",
    "cvepdb/laravel-menus": "dev-master",
    "cvepdb/laravel-settings": "dev-master",
    "cvepdb/laravel-addresses": "dev-master",
    "cvepdb-cms/module-settings": "dev-master",
    "cvepdb-cms/module-environments": "dev-master",
    "cvepdb-cms/module-files": "dev-master",
    "cvepdb-cms/module-installer": "dev-master",
    "cvepdb-cms/module-themes": "dev-master",
    "cvepdb-cms/module-users": "dev-master",
    "cvepdb-cms/theme-adminlte": "dev-master",
    "cvepdb-cms/theme-lumen": "dev-master",
    "barryvdh/laravel-elfinder": "^0.3.8",
    "dropbox-php/dropbox-php": "^1.0",
    "nao-pon/flysystem-google-drive": "~1.1",
    "google/apiclient": "~2.0@rc",
    "nao-pon/elfinder-flysystem-driver-ext": "^1.0",
    "league/pipeline": "^0.3.0",
    "simplepie/simplepie": "^1.4",
    "cvepdb-cms/module-clashofclan" : "dev-master",
    "toniperic/php-clash-of-clans": "dev-master",
    "cvepdb-cms/module-csgolounge" : "dev-master",
    "cvepdb-cms/module-servers" : "dev-master",
    "cvepdb-cms/module-steam" : "dev-master",
    "invisnik/laravel-steam-auth": "^2.0",
    "xpaw/php-source-query-class": "^1.0",
    "da-mitchell/steam-api": "^2.1",
    "cvepdb-cms/module-teams" : "dev-master",
    "cvepdb-cms/module-tournaments" : "dev-master",
    "cvepdb/theme-longwave" : "dev-master"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "mockery/mockery": "0.9.*",
    "league/factory-muffin": "^3.0",
    "league/factory-muffin-faker": "^1.0",
    "codeception/codeception": "2.2.1",
    "codeception/c3": "2.*",
    "phpunit/php-code-coverage": "^3",
    "deployer/deployer": "3.2.*",
    "site5/phantoman": "^1.1",
    "jonnyw/php-phantomjs": "^4.3",
    "jakoch/phantomjs-installer": "^2.1",
    "captbaritone/mailcatcher-codeception-module": "^1.1",
    "codeception/phpbuiltinserver": "^1.2",
    "redcomponent/teststatistics": "dev-master",
    "blackfire/php-sdk": "^1.5",
    "antennaio/laravel-codeception-dbdump": "^0.0.5",
    "squizlabs/php_codesniffer": "^2.7",
    "pragmarx/laravelcs": "^0.1.0",
    "sebastian/phpcpd": "^2.0",
    "barryvdh/laravel-debugbar": "^2.2",
    "barryvdh/laravel-ide-helper": "^2.1"
  },
  "autoload": {
    "classmap": [
      "database"
    ],
    "psr-4": {
      "cms\\": "app/",
      "cms\\Modules\\": "modules/"
    },
    "files": [
      "app/App/Services/BackendNavigationsHelpers.php",
      "app/App/Services/BackendPaginationsHelpers.php",
      "app/App/Services/MCryptHelpers.php",
      "app/App/Services/helpers.php"
    ]
  },
  "autoload-dev": {
    "classmap": [
    ]
  },
  "scripts": {
    "post-root-package-install": [
      "php -r \"file_exists('.env.installer') || copy('.env.example', '.env.installer');\""
    ],
    "post-create-project-cmd": [
      "php artisan key:generate"
    ],
    "post-install-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postInstall",
      "php artisan optimize"
    ],
    "post-update-cmd": [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate",
      "php artisan optimize"
    ],
    "before-test": [
      "mkdir -p storage/app",
      "mkdir -p storage/logs",
      "mkdir -p storage/framework/cache",
      "mkdir -p storage/framework/sessions",
      "mkdir -p storage/framework/views",
      "chmod -R 777 storage",
      "php -r \"file_exists('.env.installer') || copy('.env.example', '.env.installer');\""
    ],
    "test": [
      "vendor/bin/codecept run --env=installer -g installer api",
      "vendor/bin/codecept run --env=installer -g installer functional --coverage",
      "vendor/bin/codecept run --env=installer -g installer unit --coverage",
      "vendor/bin/codecept run --env=installer -g installer acceptance",
      "echo 'testing' > .env && cp tests/_data/.env.testing .",
      "vendor/bin/codecept run -g core api",
      "vendor/bin/codecept run -g core functional --coverage",
      "vendor/bin/codecept run -g core unit --coverage",
      "vendor/bin/codecept run -g core acceptance",
      "vendor/bin/codecept run -g installed api",
      "vendor/bin/codecept run -g installed functional --coverage",
      "vendor/bin/codecept run -g installed unit --coverage",
      "vendor/bin/codecept run -g installed acceptance"
    ],
    "test-windows-firefox": [
      "call bin\\codecept run --env=installer,firefox -g installer acceptance",
      "echo 'testing' > .env && cp tests/_data/.env.testing .",
      "call bin\\codecept run --env=firefox -g core acceptance",
      "call bin\\codecept run --env=firefox -g installed acceptance"
    ],
    "test-windows-ie": [
      "call bin\\codecept run --env=installer,ie -g installer acceptance",
      "echo 'testing' > .env && cp tests/_data/.env.testing .",
      "call bin\\codecept run --env=ie -g core acceptance",
      "call bin\\codecept run --env=ie -g installed acceptance"
    ],
    "test-windows-chrome": [
      "call bin\\codecept run --env=installer,chrome -g installer acceptance",
      "echo 'testing' > .env && cp tests/_data/.env.testing .",
      "call bin\\codecept run --env=chrome -g core acceptance",
      "call bin\\codecept run --env=chrome -g installed acceptance"
    ],
    "test-phpcs": [
      "vendor/bin/phpcs --standard=$PWD/vendor/pragmarx/laravelcs/Standards/Laravel/ app"
    ],
    "test-phpcs-duplicated": [
      "vendor/bin/phpcpd app"
    ]
  },
  "config": {
    "preferred-install": "dist"
  },
  "extra": {
    "installer-paths": {
      "modules/{$name}": [
        "cvepdb-cms/module-settings",
        "cvepdb-cms/module-environments",
        "cvepdb-cms/module-installer",
        "cvepdb-cms/module-themes",
        "cvepdb-cms/module-files",
        "cvepdb-cms/module-users",
        "cvepdb-cms/module-clashofclan",
        "cvepdb-cms/module-csgolounge",
        "cvepdb-cms/module-servers",
        "cvepdb-cms/module-steam",
        "cvepdb-cms/module-teams",
        "cvepdb-cms/module-tournaments"
      ],
      "resources/themes/{$name}": [
        "cvepdb-cms/theme-adminlte",
        "cvepdb-cms/theme-lumen",
        "cvepdb/theme-longwave"
      ]
    }
  }
}
